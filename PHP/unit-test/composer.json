{
    "name": "ujun-al-rantaui/unit-test",
    "description": "belajar unit test dari channel youtube Programmer Zaman Now - 31-12-2023",
    "type": "project",
    "autoload": {
        "psr-4": {
            "UjunAlRantaui\\UnitTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UjunAlRantaui\\UnitTest\\": "test/"
        }
    },
    "authors": [
        {
            "name": "UJUN-Al-Rantaui",
            "email": "ujun.muhammadjunaidi@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "10.5.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml --testsuite all",
        "test:counter": "vendor/bin/phpunit --configuration phpunit.xml --testsuite counter",
        "test:product": "vendor/bin/phpunit --configuration phpunit.xml --testsuite product"
    }
}
